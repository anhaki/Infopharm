<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:shape="rectangle"
    tools:ignore="MissingDefaultResource">

    <solid
        android:color="#73B8EB" >
    </solid>

    <stroke
        android:width="3dp"
        android:color="@color/black" >
    </stroke>

    <padding
        android:left="15dp"
        android:top="15dp"
        android:right="15dp"
        android:bottom="15dp"    >
    </padding>

    <corners
        android:radius="11dp"   >
    </corners>

</shape>
